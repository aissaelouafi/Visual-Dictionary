                                    <img src="./contents/images/portfolios/600x600/1.jpg" alt="">
                                  <div class="cbp-caption-activeWrap">
                                      <div class="cbp-l-caption-alignCenter">
                                          <div class="cbp-l-caption-body">
                                              <div class="btn-group">
                                                  <a href="/topic?name={{topic.topic}}&page={{ topic.page }}" class="cbp-singlePage btn text-lowercase" rel="nofollow">more info</a>
                                                  <a href="/topic?name={{topic.topic}}&page={{ topic.page }}" class="cbp-singlePage btn text-lowercase" rel="nofollow" data-cbp-singlePage="projects">more info</a>
                                              </div>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                              <div class="cbp-l-grid-projects-title uppercase text-center uppercase text-center">{{ topic.topic }}</div>
                              <div class="cbp-l-grid-projects-desc uppercase text-center uppercase text-center">Page : {{ topic.page }}</div>
                          </div>
                      </div>



                      <!--/portfolio 1-->
                  </div>
